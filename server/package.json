{
  "name": "fitness-tracker-server",
  "version": "1.0.0",
  "description": "Backend for Fitness Tracker App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm install && npm run prepare",
